{
  "version": 2,
  "name": "Migrate Dashboard",
  "alias": "migrate-dashboard-staging",
  "builds": [
    { "src": "./out", "use": "@now/static" },
    { "src": "./static", "use": "@now/static" }
  ],
  "routes": [
    {"src": "^/favicon.ico$", "dest": "/static/favicon.ico"},
    { "src": "/(.*)", "headers": {"cache-control": "s-maxage=0"}, "dest": "/index.html" }
  ]
}
